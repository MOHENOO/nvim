{
  "configurations": {
    "run": {
      "adapter": "vscode-go",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/main.go",
        "mode": "debug",
        "dlvToolPath": "$HOME/Development/go/bin/dlv",
        "trace": true,
        "env": { "GO111MODULE": "on" }
      }
    }
  }
}
